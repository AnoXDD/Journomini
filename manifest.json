{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Journomni",
  "description": "Anoxic's own omnibox twist",
  "version": "0.2.1",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "omnibox": { "keyword": "j" },
  "permissions": [
    "storage",
    "webRequest",
    "*://*/*",
    "notifications",
    "webRequestBlocking"
  ],
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "script.js" ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "icon.png", "success.ogg"
  ],
  "manifest_version": 2
}
