{
  "name": "Journomini",
  "description": "Anoxic's own ominibox twist",
  "version": "0.3",
  "browser_action": {
    "default_title": "Sign in",
    "default_popup": "./popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "j"
  },
  "permissions": [
    "storage",
    "webRequest",
    "*://*/*",
    "notifications",
    "webRequestBlocking",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "script.js",
        "jquery.min.js",
        "material.min.js"
      ],
      "run_at": "document_end",
      "match_about_blank": true
    }
  ],
  "web_accessible_resources": [
    "icon.png",
    "success.ogg",
    "jquery.min.js",
    "material.min.js",
    "material_icons.css",
    "material_icons.woff2"
  ],
  "options_page": "options.html",
  "manifest_version": 2
}
