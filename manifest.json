{
  "name": "Journomini",
  "description": "Anoxic's own ominibox twist",
  "version": "0.5.2",
  "browser_action": {
    "default_title": "Fetcher",
    "default_popup": "./popup.html"
  },
  "background": {
    "scripts": [
      "static/js/lib/jquery.min.js",
      "static/js/background.js"
    ]
  },
  "omnibox": {
    "keyword": "j"
  },
  "permissions": [
    "storage",
    "webRequest",
    "*://*/*",
    "notifications",
    "webRequestBlocking",
    "tabs",
    "geolocation"
  ],
  "icons":{
    "192": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "static/js/script.js"
      ],
      "run_at": "document_end",
      "match_about_blank": true
    }
  ],
  "web_accessible_resources": [
    "static/icon.png",
    "static/success.ogg"
  ],
  "options_page": "options.html",
  "manifest_version": 2
}
