{
  "name": "Journomini",
  "description": "Anoxic's own ominibox twist",
  "version": "0.1",
  "browser_action": {
    "default_title": "Sign in",
    "default_popup": "./popup.html"
  },
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "omnibox": { "keyword": "j" },
  "permissions": [
    "storage",
    "webRequest",
    "*://*/*",
    "notifications",
    "webRequestBlocking"
  ],
  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "script.js" ],
      "run_at": "document_end"
    }
  ],
  "manifest_version": 2
}
