{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Journomni",
  "description": "Anoxic's own omnibox twist",
  "version": "0.3",
  "background": {
    "scripts": [
      "jquery.min.js",
      "background.js"
    ]
  },
  "omnibox": {
    "keyword": "j"
  },
  "permissions": [
    "storage",
    "webRequest",
    "*://*/*",
    "notifications",
    "webRequestBlocking",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_end",
      "all_frames" : true,
      "match_about_blank": true
    }
  ],
  "web_accessible_resources": [
    "icon.png",
    "success.ogg"
  ],
  "manifest_version": 2
}
